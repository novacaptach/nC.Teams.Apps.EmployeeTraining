{
  "AzureAd": {
    "Instance": "<<AzureAd.Instance>>",
    "TenantId": "<<AzureAd.TenantId>>",
    "ApplicationIdURI": "<<AzureAd.ApplicationIdURI>>",
    "ValidIssuers": "https://login.microsoftonline.com/TENANT_ID/v2.0,https://sts.windows.net/TENANT_ID/",
    "GraphScope": "openid profile offline_access https://graph.microsoft.com/User.ReadBasic.All https://graph.microsoft.com/People.Read https://graph.microsoft.com/Directory.Read.All https://graph.microsoft.com/Calendars.ReadWrite"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "<<ApplicationInsights.InstrumentationKey>>",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information"
    }
  },
  "App": {
    "AppBaseUri": "<<App.AppBaseUri>>",
    "ManifestId": "<<App.ManifestId>>",
    "CacheDurationInMinutes": 60,
    "EventsPageSize": 30
  },
  "i18n": {
    "DefaultCulture": "en",
    "SupportedCultures": "en,ar,de,es,fr,he,ja,ko,pt-BR,ru,zh-CN,zh-TW"
  },
  "RetryPolicy": {
    "medianFirstRetryDelay": 1000,
    "retryCount": 2
  },
  "KeyVault": {
    "BaseURL": "<<KeyVault.BaseURL>>"
  },
  "KeyVaultStrings": {
    "StorageConnection": "StorageConnection--SecretKey",
    "MicrosoftAppId": "MicrosoftAppId--SecretKey",
    "MicrosoftAppPassword": "MicrosoftAppPassword--SecretKey",
    "SearchServiceName": "SearchServiceName--SecretKey",
    "SearchServiceAdminApiKey": "SearchServiceAdminApiKey--SecretKey",
    "SearchServiceQueryApiKey": "SearchServiceQueryApiKey--SecretKey"
  },
  "DeploymentType": {
    "IsGCCHybrid": false
  }
}